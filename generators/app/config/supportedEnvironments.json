      "jdbcVersion": "6.2.2.jre",
      "jdbcClass": "com.microsoft.sqlserver.jdbc.SQLServerXADataSource",
      "jbossModule": "com.microsoft.sqlserver.jdbc",
      "jbossPath": "com/microsoft/sqlserver/jdbc/main/"
