      "jdbcLink": "https://repo1.maven.org/maven2/org/postgresql/postgresql/%s/postgresql-%s.jar",
      "jdbcVersion": "42.2.6",
      "jdbcClass": "org.postgresql.Driver",
      "jbossModule": "org.postgresql",
      "jbossPath": "org/postgresql/main/"
