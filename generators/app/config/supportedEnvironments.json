      "jbossPath": "com/mysql/jdbc/main/"
    },
    "MariaDB": {
      "versions": ["10.0", "10.2", "10.3"],
      "defaultDbName": "process-engine",
      "defaultPort": "3306",
      "dbString": "jdbc:mariadb://%s:%s/%s",
