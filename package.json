{
  "name": "generator-camunda",
  "version": "0.0.3",
  "description": "A Yeoman generator for the Camunda BPM Platform",
  "homepage": "https://github.com/koevskinikola/generator-camunda",
  "author": {
    "name": "Nikola Koevski",
    "email": "koevskin@gmail.com",
    "url": "https://github.com/koevskinikola"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "camunda",
    "bpm",
    "workflow engine",
    "application server"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "prettier": "^1.15.2",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-config-xo": "^0.25.0",
    "jest": "^29.3.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "download": "^7.1.0",
    "sprintf-js": "^1.1.2",
    "xml-js": "^1.6.11",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "koevskinikola/generator-camunda",
  "license": "Apache-2.0"
}
